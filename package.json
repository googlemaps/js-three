{
  "name": "@googlemaps/three",
  "version": "1.0.0",
  "keywords": [
    "google",
    "maps",
    "webgl",
    "threejs"
  ],
  "homepage": "https://github.com/googlemaps/js-three",
  "bugs": {
    "url": "https://github.com/googlemaps/js-three/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/googlemaps/js-three.git"
  },
  "license": "Apache-2.0",
  "author": "Justin Poehnelt",
  "files": [
    "dist/*"
  ],
  "main": "dist/index.umd.js",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "docs": "typedoc src/index.ts",
    "format": "prettier *.json *.js e2e/* --write && eslint src/* --fix",
    "lint": "eslint src/*",
    "prepare": "rollup -c",
    "test": "jest src/*",
    "test:e2e": "jest --passWithNoTests e2e/*",
    "build": "rm -rf dist && rollup -c && npm run build:examples",
    "build:examples": "rm -rf public && rollup -c rollup.config.examples.js",
    "start": "rollup -c rollup.config.examples.js -w"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@babel/runtime-corejs3": "^7.15.3",
    "@googlemaps/jest-mocks": "^1.2.0",
    "@googlemaps/js-api-loader": "^1.12.1",
    "@rollup/plugin-html": "^0.2.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/d3-random": "^3.0.1",
    "@types/google.maps": "^3.45.6",
    "@types/jest": "^27.0.1",
    "@types/proj4": "^2.5.0",
    "@types/selenium-webdriver": "^4.0.14",
    "@types/stats.js": "^0.17.0",
    "@types/three": "^0.131.0",
    "@typescript-eslint/eslint-plugin": ">=4.29.0",
    "@typescript-eslint/parser": ">=4.28.3",
    "chromedriver": "^92.0.1",
    "core-js": "^3.16.1",
    "d3-random": "^3.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.1",
    "geckodriver": "^2.0.1",
    "jest": "^26.6.3",
    "prettier": "^2.3.2",
    "rollup": "^2.54.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "selenium-webdriver": "^4.0.0-beta.4",
    "ts-jest": "^26.5.6",
    "typedoc": "^0.21.5",
    "typescript": "^4.3.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "dependencies": {
    "three": "^0.131.3"
  }
}
