{
  "name": "@googlemaps/three",
  "version": "1.0.10",
  "keywords": [
    "google",
    "maps",
    "webgl",
    "threejs"
  ],
  "homepage": "https://github.com/googlemaps/js-three",
  "bugs": {
    "url": "https://github.com/googlemaps/js-three/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/googlemaps/js-three.git"
  },
  "license": "Apache-2.0",
  "author": "Justin Poehnelt",
  "main": "dist/index.umd.js",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build:examples": "rm -rf public && rollup -c rollup.config.examples.js",
    "docs": "typedoc src/index.ts && npm run build:examples && cp -r dist docs/dist && cp -r public docs/public",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "prepare": "rm -rf dist && rollup -c",
    "start": "rollup -c rollup.config.examples.js -w",
    "test": "jest src/*",
    "test:e2e": "jest --passWithNoTests e2e/*"
  },
  "dependencies": {
    "three": "^0.135.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/runtime-corejs3": "^7.16.3",
    "@googlemaps/jest-mocks": "^1.3.9",
    "@googlemaps/js-api-loader": "^1.12.11",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/d3-random": "^3.0.1",
    "@types/google.maps": "^3.46.1",
    "@types/jest": "^27.0.3",
    "@types/proj4": "^2.5.2",
    "@types/selenium-webdriver": "^4.0.16",
    "@types/stats.js": "^0.17.0",
    "@types/three": "^0.134.0",
    "@typescript-eslint/eslint-plugin": ">=4.33.0",
    "@typescript-eslint/parser": ">=4.33.0",
    "chromedriver": "^96.0.0",
    "core-js": "^3.19.1",
    "d3-random": "^3.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "geckodriver": "^2.0.4",
    "jest": "^26.6.3",
    "prettier": "^2.5.0",
    "rollup": "^2.60.2",
    "rollup-plugin-terser": "^7.0.2",
    "selenium-webdriver": "^4.1.0",
    "ts-jest": "^26.5.6",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://wombat-dressing-room.appspot.com"
  }
}
